#+name: Question 1
#+BEGIN_SRC R :results value :file myplot.png :exports both :session test
  birthwt <- birthwt %>%
  mutate(race = factor(race, labels=c("White","Black","Other")),
         smoke = factor(smoke, labels=c("No Smoke", "Smoke")))

  plot_1 <- ggplot(birthwt, aes(x=bwt)) + geom_histogram()
  plot_save <- ggsave("myplot.pdf", plot = plot_1)
  
#+END_SRC

